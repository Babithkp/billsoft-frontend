{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "billsoft",
  "version": "0.1.3",
  "identifier": "com.babith.app",
  "build": {
    "beforeDevCommand": "bun cross-env BROWSER=none bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Billsoft",
        "minWidth": 800,
        "minHeight": 600,
        "width": 1400,
        "height": 700
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "publisher": "Trikona Tech",
    "createUpdaterArtifacts": true,
    "targets": [
      "nsis",
      "msi"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "windows": {
        "installMode": "basicUi"
      },
      "active": true,
      "endpoints": [
        "https://github.com/Babithkp/billsoft-frontend/releases/latest/download/latest.json"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDk0NUY4QkY1Qjc2RTJCNTgKUldSWUsyNjM5WXRmbE5mcnIySmEyc0NCV21KLytHZk0yRnRpNXRsQksrODhVbGdlSUNER1JlYnEK"
    }
  }
}